{
  "name": "pacman-engine",
  "version": "0.1.6",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev:frontend": "vite",
    "host:frontend": "vite --host",
    "dev:backend": "tsx src/backend/index",
    "dev": "concurrently --raw -n SERVER,VITE \"npm run dev:backend\" \"npm run dev:frontend\"",
    "host": "concurrently --raw -n SERVER,VITE \"npm run dev:backend\" \"npm run host:frontend\""
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "dotenv": "^17.2.3",
    "typescript": "^5.5.3",
    "vite": "^7.2.2"
  },
  "private": true,
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "concurrently": "^9.2.1",
    "express": "^5.1.0"
  }
}
